{{> header}}
{{> navbar}}

<div class="container mt-4">
  <div class="row">
    <div class="col-lg-4"></div>
    <div class="col-lg-4 mt-3">
      <div class="card mb-3">
        <div class="card-header">
          Add Kabupaten
        </div>
        <div class="card-body">
          <form method="POST" action="/add-kabupaten" class="p-4" enctype="multipart/form-data">
            <div class="mb-3">
              <label for="kabupaten" class="form-label">Kabupaten</label>
              <input type="text" class="form-control" id="kabupaten" name="namaKab">
            </div>
            <div class="mb-3">
              <label for="heroess" class="form-label">Provinsi</label>
              <select class="form-select" name="provinsi">
                <option disabled selected>Open this select menu</option>
                {{#each provinsies}}
                <option value="{{this.id}}">{{this.nama_prov}}</option>
                {{/each}}
              </select>
            </div>
            <div class="mb-3">
              <label for="Photo" class="form-label">Diresmikan</label>
              <input type="date" class="form-control" id="Photo" name="diResmikan">
            </div>
            <div class="mb-3">
              <label for="Photo" class="form-label">Photo</label>
              <input type="file" class="form-control" id="Photo" name="photo">
            </div>
            <button type="submit" class="btn btn-primary mt-3">Submit</button>
          </form>
        </div>
      </div>
    </div>
    <div class="col-lg-4"></div>
  </div>
</div>

{{> footer}}